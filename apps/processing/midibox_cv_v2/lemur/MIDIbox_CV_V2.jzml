else if( x == 28 ) RateM2.value = '1/16';
else if( x == 29 ) RateM2.value = '1/32.';
else if( x == 30 ) RateM2.value = '1/16T';
else if( x == 31 ) RateM2.value = '1/48';
else RateM2.value = '---';

}
